public class DogBarkTest
{
	public static void main(String[] args) 
	{
		DogBark myPuppy = new DogBark();
		myPuppy.name = "Happy";
		myPuppy.hungry = 100;

		if(args.length != 0){
			for(int i=0; i<args.length; i++){
				int cmd = Integer.parseInt(args[i]);
				switch(cmd){
					case 0:
						myPuppy.bark();
						break;
					case 1:
						myPuppy.eat();
						break;
				}	
			}

		}
		
	}
}
